/* components/smallCharCard/smallCharCard.wxss */
smallCharCard {
	width: 250rpx;
  padding: 15rpx;
  background-color: white;
	border-radius: 37.5rpx;
  box-shadow: 5rpx 5rpx 5rpx 5rpx gray;
  justify-content: space-around;
  margin: 30rpx;
}

.card {
	display: flex;
	flex-direction: row;
	width: 200rpx;
}
.name{
  position: relative;
  height: 50rpx;
	font-size: 35rpx;
	font-weight: normal;
	text-align: center;
}
